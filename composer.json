{
    "name": "bytes-commerce/zabbix-api",
    "description": "A PHP library for interacting with the Zabbix API using JSON-RPC 2.0",
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Maximilian Graf Schimmelmann",
            "email": "office@bytes-commerce.de"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "BytesCommerce\\Zabbix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BytesCommerce\\Zabbix\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.3",
        "symfony/cache": "^7",
        "symfony/config": "^7",
        "symfony/dependency-injection": "^7",
        "symfony/http-client": "^7",
        "symfony/http-kernel": "^7",
        "psr/log": "^3",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.22",
        "rector/rector": "^2.0",
        "phpstan/phpstan": "^2.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-text",
        "analyse": "vendor/bin/phpstan analyse",
        "check": [
            "@analyse",
            "@test"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
